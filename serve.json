{
    "headers": [
      {
        "source" : "**/*.@(jpg|jpeg|gif|png)",
        "headers" : [
            {
                "key" : "Cache-Control",
                "value" : "max-age=7200"
            },
            {
                "key" : "Access-Control-Allow-Origin",
                "value" : "*"
            }
        ]
      }, {
        "source" : "404.html",
        "headers" : [{
          "key" : "Cache-Control",
          "value" : "max-age=300"
        }]
      }
    ]
  }